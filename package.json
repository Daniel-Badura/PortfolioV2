{
  "name": "portfolio",
  "version": "2.0.0",
  "description": "Daniel Badura new Portfolio",
  "main": "server.ts",
  "type": "module",
  "scripts": {
    "start": "ts-node --esm backend/server.js",
    "server": "nodemon --exec ts-node-esm backend/server",
    "client": "npm start --prefix frontend",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "data:feed": "node backend/seeder",
    "data:remove": "node backend/seeder -d",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix frontend && npm run build --prefix frontend",
    "test": "jest --forceExit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Daniel-Badura/PortfolioV2.git"
  },
  "keywords": [
    "portfolio",
    "daniel.badura"
  ],
  "author": "Daniel Badura",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Daniel-Badura/PortfolioV2/issues"
  },
  "homepage": "https://daniel-badura-portfolio.herokuapp.com/",
  "devDependencies": {
    "@types/express": "^4.17.18",
    "@types/morgan": "^1.9.6",
    "colors": "^1.4.0",
    "concurrently": "^8.2.1",
    "morgan": "^1.10.0",
    "nodemon": "^3.0.1",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "bcrypt": "^5.1.1",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-async-handler": "^1.2.0",
    "jest": "^29.7.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^7.5.3",
    "nodemailer": "^6.9.5"
  }
}